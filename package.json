{
  "name": "simple-ts-react-app",
  "version": "1.0.3",
  "description": "",
  "main": ".dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "coverage": "cross-env NODE_ENV=test jest --coverage",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js --progress --profile --color --display-error-details --display-cached",
    "prebuild": "yarn clean",
    "lint": "tslint \"src/**/*.ts?(x)\"",
    "lint:fix": "tslint \"src/**/*.ts?(x)\" --fix",
    "open:src": "cross-env NODE_ENV=development node tools/srcServer.js",
    "open:dist": "node tools/distServer.js",
    "startFinal": "npm-run-all --parallel test:watch open:src lint:watch",
    "start": "yarn run open:src",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "yarn test -o --watch"
  },
  "jest": {
    "setupFiles": [
      "./jest.setup.js"
    ],
    "moduleDirectories": [
      "./node_modules",
      "./src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "typescript-babel-jest"
    },
    "testMatch": [
      "**/*.test.(ts|tsx|js)"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.1",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "@babel/register": "7.0.0",
    "@types/enzyme": "^3.1.11",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/history": "4.7.2",
    "@types/jest": "^23.3.9",
    "@types/react": "^16.4.2",
    "@types/react-dom": "^16.0.6",
    "@types/react-hot-loader": "4.1.0",
    "@types/react-redux": "7.0.0",
    "@types/react-router-dom": "^4.2.7",
    "@types/redux-immutable-state-invariant": "2.1.0",
    "@types/webpack-env": "^1.13.6",
    "autoprefixer": "9.4.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "browser-sync": "2.26.3",
    "chalk": "2.4.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.1",
    "cssnano": "4.1.8",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "file-loader": "^2.0.0",
    "hard-source-webpack-plugin": "0.13.1",
